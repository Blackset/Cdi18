package ExoObjet;

public class Individu
{
		 private String Nom;
		 private String Prénom;
		 private String Adresse;
		public String getNom()
		{
			return Nom;
		}
		public void setNom(String nom)
		{
			Nom = nom;
		}
		public String getPrénom()
		{
			return Prénom;
		}
		public void setPrénom(String prénom)
		{
			Prénom = prénom;
		}
	 
	 
}
